.main_wrap{width:100%;height:100%;}
.flex_start{display:flex;justify-content:flex-start;align-items:center;}

.header{background:#3a3e47;height:54px;position:relative;z-index:101;}
.header_right{width:190px;float:right;display:flex;align-items:center;justify-content:space-between;padding-right:10px;height:100%;position:relative;}
.header_left{width:270px;float:left;display:flex;align-items:center;justify-content:flex-start;height:100%;}
.header .navs{margin-left:280px;margin-right:240px;}

.navs{text-align:center;display:flex;display:-webkit-flex;justify-content:center;}
.navs .icon{width:72px;height:54px;display:block;display:flex;display:-webkit-flex;justify-content:center;align-items:center;margin-right:10px;}
.navs .icon:hover{background:#444850;}
.navs .icon.select{background:#53575f;}
.navs .icon i{opacity:.5;display:block;width:20px;height:20px;}
.navs .icon.select i{opacity:1;}
.navs .chat i{width:22px;background:url(../img/icons.png) no-repeat 0px -169px;}
.navs .concat i{background:url(../img/icons.png) no-repeat -81px -169px;}

.header_right .avatar{width:26px;height:26px;overflow:hidden;border-radius:4px;}
.header_right .avatar img{width:100%;height:100%;}
.header_right .win_btns{float:right;margin-right:12px;margin-top:-2px;}
.header_right .win_btns .win_btn{display:inline-block;margin-left:10px;vertical-align:middle;}
.header_right .win_btns .win_btn i{height:11px;width:11px;display:block;background:url(../img/icons.png) no-repeat;}
.header_right .win_btns .win_min i{background-position:-34px -282px;}
.header_right .win_btns .win_min:hover i{background-position:-45px -282px;}
.header_right .win_btns .win_min:active i{background-position:-56px -282px;}
.header_right .win_btns .win_max i{background-position:0px -282px;}
.header_right .win_btns .win_max:hover i{background-position:-11px -282px;}
.header_right .win_btns .win_max:active i{background-position:-22px -282px;}
.header_right .win_btns .win_reset i{background-position:0px -282px;}
.header_right .win_btns .win_reset:hover i{background-position:-11px -282px;}
.header_right .win_btns .win_reset:active i{background-position:-22px -282px;}
.header_right .win_btns .win_close i{width:9px;height:9px;background-position:0px -209px;}
.header_right .win_btns .win_close:hover i{background-position:-9px -209px;}
.header_right .menu_drop{position:absolute;top:54px;left:20px;background:#3d414a;border-radius:0 0 8px 8px;box-shadow:0 0 6px #000;width:155px;overflow:hidden;}
.header_right .menu_drop a{display:block;color:#bbb;padding-left:16px;line-height:40px;text-decoration:none;padding-right:20px;font-size:13px;}
.header_right .menu_drop a:hover{color:#fff;background:#33363e;}
.header_right .menu_drop .spliter{border-bottom:1px solid #33363e;}

.menu{display:inline-block;background:url(../img/icons.png) no-repeat 0 -300px;width:16px;height:14px;margin-left:10px;}
.menu:hover{background-position:-16px -300px;}
.menu:active{background-position:-32px -300px;}


.header_left .main_logo{margin-left:22px;margin-right:18px;}
.search{width:150px;position:relative;background:#494d55;border-radius:4px;height:28px;}
.search input{background:none;border:0;padding-right:20px;width:100%;box-sizing:border-box;font-size:14px;color:#aaadb3;line-height:28px;padding-left:13px;}
.search input::-webkit-input-placeholder{color:#797d84;}
.search a{position:absolute;width:12px;height:12px;background:url(../img/icons.png) no-repeat -14px -318px;top:8px;right:8px;}
.search a.s_i{background-position:-14px -318px;}
.search a.clear_ipt{background-position:0 -226px;}
.search a.clear_ipt:hover{background-position:-12px -226px;}
.search a.clear_ipt:active{background-position:-24px -226px;}
.header_left .start_chat{display:block;width:26px;height:26px;background:url(../img/icons.png) no-repeat 0 -249px;margin-left:12px;}
.header_left .start_chat:hover{background-position:-26px -249px;}
.header_left .search .search_res{position:absolute;top:40px;width:260px;left:-20px;background:#3a3e47;overflow:auto;max-height:500px;min-height:110px;}

.header_left .search .search_res::-webkit-scrollbar-thumb{background: #3a3e47;}
.header_left .search:hover .search_res::-webkit-scrollbar-thumb{background:#595e6c;}
.search_res .search_cate:first-child .search_cate_title{padding-top:0;}
.header_left .search .no_res{background:url(../img/searchEmptyShow.png) no-repeat center center #3a3e47;min-height:150px;}
.header_left .search .search_res .search_avatar{width:38px;height:38px;float:left;border-radius:4px;}
.header_left .search .search_res .res_item::after{clear:both;display:block;content:' ';width:0;height:0;font-size:0;line-height:0;}
.header_left .search .search_res .item_info{margin-left:44px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;margin-right:10px;}
.header_left .search .search_res .item_info p{white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
.res_item{color:#fff;font-size:13px;padding:10px 0px 10px 20px;}
.res_item p.grap{color:#a0a0a0;font-size:12px;margin-right:10px;}
.res_item:hover{background:#33363e;}
.res_item.active{background:#24262c;}
.search_cate_title{color:#67696e;padding:10px 20px;}
.search_cate::after{display:block;content:' ';margin-left:20px;border-bottom:1px solid #33363e;height:0;}

.left_part{position:fixed;top:54px;left:0px;bottom:0px;min-width:252px;overflow:hidden;background:#ebecf0;z-index:99;width:252px;}
.right_part{position:fixed;left:252px;top:54px;bottom:0px;right:0;background:#f1f2f4;z-index:100;}
.right_part.no_content{background:url(../img/emptyShow.png) no-repeat center center #f1f2f4;}
.right_part .chat_wrap,.right_part .content_detail{position:absolute;top:0px;bottom:0;left:0;right:0;display:none;z-index:99;}
.right_part .content_detail{background:#f1f2f4;}
.right_part .content_detail.no_content{background:url(../img/emptyShow.png) no-repeat center center #f1f2f4;}
.right_part .chat_wrap .chat_head{height:45px;}
.right_part .chat_wrap .chat_head .inner{padding:0 20px;margin:0 25px;border-bottom:1px solid #e1e3e5;height:44px;display:flex;justify-content:center;align-items:center;position:relative;z-index:112;}
.right_part .chat_wrap .chat_head .inner .chat_name{max-width:70%;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;font-size:16px;}
.right_part .chat_wrap .chat_head .inner .chat_title{cursor:pointer;display:flex;justify-content:center;align-items:center;width:100%;}
.right_part .chat_wrap .chat_head .inner .chat_drop{transform: rotate(180deg);display:inline-block;width:14px;height:10px;background:url(../img/icons.png) no-repeat 0 -350px;margin-left: 4px;margin-top:2px;}
.right_part .chat_wrap .chat_head .inner .chat_drop.showing{transform: rotate(0deg);margin-top:4px;}
.right_part .chat_wrap .group_info_warp{z-index:998;}
.group_info_warp .group_info_inner{position:absolute;left:1px;top:45px;right:0;z-index:999;}
