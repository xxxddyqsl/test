.history_right .msg .history_time ._msg_time{margin-left:15px;-webkit-user-select: none;    -moz-user-select: none;}

.history_right .msg .msg_txt_warper .warper{max-width:100%;}
.history_right .msg{padding-bottom:4px;}
.history_right .msg.select{background:#e3efff;}
