.img_preview_warp .quick_next{z-index:99999;display:block;width:38px;height:38px;background:url(../img/next_ent.png) no-repeat center center #fff;position:absolute;top:50%;margin-top:-19px;right:10px;display:none;border-radius:20px;}
.img_preview_warp .quick_prev{z-index:99999;display:block;width:38px;height:38px;background:url(../img/prev_ent.png) no-repeat center center #fff;position:absolute;top:50%;margin-top:-19px;left:10px;display:none;border-radius:20px;}


.thumb_wrap{position:absolute;bottom:-10000px;right:-10000px;width:100px;height:100px;background:rgba(0,0,0,.5);z-index:99999999;}
.thumb_wrap .toper{position:absolute;top:50%;left:50%;/* background:rgba(0,0,0,.5); */}
.thumb_wrap .toper .mask{position:absolute;top:0;left:0;right:0;bottom:0;background:rgba(0,0,0,.5);}
.thumb_wrap .inner{position:absolute;border:1px solid #02e800;box-sizing:border-box;}
.thumb_wrap .inner .border_div{position:absolute;}
.thumb_wrap .content_canvas{position:absolute;top:0;left:0;}

.global_mask{position:absolute;top:0;left:0;right:0;bottom:0;z-index:9999999;}
.global_mask .roate_loading{position:absolute;top:50%;left:50%;width:48px;height:48px;margin-left:-24px;margin-top:-24px;background:url(../img/loading.png) no-repeat center center;animation: roate 1s linear infinite;opacity:.5;}
@keyframes roate{
    from {transform: rotate(0deg);}
    to {transform: rotate(360deg);}
}
.resize{background:url(../img/resize.png) no-repeat center center;background-size:85%;padding:2px;width:12px;height:12px;position:fixed;right:0px;bottom:0px;z-index:9999999;opacity:0.6;cursor:default;}

