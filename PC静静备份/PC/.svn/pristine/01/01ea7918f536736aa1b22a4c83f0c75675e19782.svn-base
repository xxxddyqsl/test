.group_info_pop{background:#fff;width:100%;border-bottom:1px solid #d9dfe6;}
.group_info_pop .group_info_title {
  font-size: 18px;
  margin: 24px 0;
  text-align: center;
}
.group_info_pop .group_info_content {
  font-size: 14px;
  background: #fff;
  padding: 0 40px;
}
.group_info_pop .group_info_content .group_info {
  padding: 15px 0 20px;
  border-bottom: solid 1px #dbdbdb;
  overflow: hidden;
  line-height: 28px;
}
.group_info_pop .group_info_content .group_info .group_div_header {
  color: #202022;
  margin-right: 15px;
  float: left;
}
.group_info_pop .group_info_content .group_info .group_div_content {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: left;
  position: relative;
}
.group_info_pop .group_info_content .group_info .group_div_content span {
  float: left;
  max-width: 70%;
  margin-right: 8px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.group_info_pop .group_info_content .group_info .group_div_content i {
  background: url(../img/icons.png) no-repeat -0px -364px;
  width: 11px;
  height: 11px;
  float: left;
  margin-top: 8px;
  cursor: pointer;
}
.group_info_pop .group_info_content .group_info .group_div_content i:hover{background-position:-11px -364px;}
.group_info_pop .group_info_content .group_info input {
  width: 99%;
  line-height: 22px;
}
.group_info_pop .group_info_content .group_info .section {
  width: 20%;
  float: left;
  overflow: hidden;
  line-height: 28px;
  min-width:145px;
}
.group_info_pop .group_info_content .group_info .section .on-off {
  width: 29px;
  height: 18px;
  border-radius: 28px;
  margin-top: -2px;
  -webkit-border-radius: 28px;
  -o-border-radius: 28px;
  -moz-border-radius: 28px;
  overflow: hidden;
  position: relative;
  cursor:pointer;
}
.group_info_pop .group_info_content .group_info .section .on-off .switch {
  background: #ffffff;
  height: 14px;
  width: 14px;
  margin: auto 1px;
  border-radius: 26px;
  -webkit-border-radius: 26px;
  -o-border-radius: 26px;
  -moz-border-radius: 26px;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  cursor: pointer;
}
.group_info_pop .group_info_content .group_info .section .off {
  background: #c8c8c8;
}
.group_info_pop .group_info_content .group_info .section .off .s_off {
  -webkit-animation: switch_off 200ms;
  -o-animation: switch_off 200ms;
  -moz-animation: switch_off;
  animation: switch_off 200ms;
  animation-fill-mode: forwards;
  -webkit-animation-fill-mode: forwards;
  -moz-animation-fill-mode: forwards;
  -o-animation-fill-mode: forwards;
}

.group_info_pop .group_info_content .group_info .section .on {
  background: #8dadf3;
}
.group_info_pop .group_info_content .group_info .section .on .s_on {
  -webkit-animation: switch_on 200ms;
  -o-animation: switch_on 200ms;
  -moz-animation: switch_on;
  animation: switch_on 200ms;
  animation-fill-mode: forwards;
  -webkit-animation-fill-mode: forwards;
  -moz-animation-fill-mode: forwards;
  -o-animation-fill-mode: forwards;
}
.group_info_pop .group_info_content .group_info .section .on-off .no_an {
  -webkit-animation-duration: 0ms;
  -o-animation-duration: 0;
  -moz-animation: ;
  animation-duration: 0;
}
.group_info_pop .group_info_content .group_members {
  max-height: 250px;
  overflow: hidden;
  padding-top: 10px;
  border-bottom: solid 1px #dbdbdb;
}
.group_info_pop .group_info_content .group_members .members_content {
  max-height: 250px;
  overflow: hidden;
}
.group_info_pop .group_info_content .group_members .members_content .section {
  float: left;
  margin: 5px;
  width: 50px;
  height: 90px;
  overflow: hidden;
  position: relative;
}
.group_info_pop .group_info_content .group_members .members_content .section .avatar {
  width: 40px;
  height: 40px;
  margin: 0 auto;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  overflow: hidden;
  cursor: pointer;
  position: relative;
}
.group_info_pop .group_info_content .group_members .members_content .section .avatar img {
  width: 100%;
  height: 100%;
}
.group_info_pop .group_info_content .group_members .members_content .section .avatar .icon_content {
  width: 100%;
  height: 100%;
}
.group_info_pop .group_info_content .group_members .members_content .section .member_name {
  color: #515865;
  font-size: 14px;
  text-align: center;
  overflow: hidden;
  word-break: break-all;
  margin-top: 6px;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.group_info_pop .group_info_content .group_members .members_content .section .group_banned{width: 40px;height: 16px;border-radius: 21px;color: #ffffff;text-align: center;line-height: 16px;font-size: 10px;background: #dbdbdb;margin: auto;}

.group_info_pop .group_info_content .group_members .members_content .section .group_admin {
  width: 40px;
  height: 16px;
  border-radius: 21px;
  -webkit-border-radius: 21px;
  -moz-border-radius: 21px;
  -ms-border-radius: 21px;
  -o-border-radius: 21px;
  color: #ffffff;
  text-align: center;
  line-height: 16px;
  font-size: 10px;
  background: #4793f6;
  margin: auto;
}
.group_info_pop .group_info_content .group_members .members_content .section .del_emp {
  position: absolute;
  top: 0;
  left: 5px;
  width: 40px;
  height: 40px;
  z-index: 10;
  background: rgba(221, 81, 41, 0.8);
  border-radius: 5px;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  overflow: hidden;
  cursor: pointer;
}
.group_info_pop .group_info_content .group_members .members_content .section .del_emp i {
  position: absolute;
  top: 20px;
  right: 10px;
  left: 10px;
  border-top: 2px solid #fff;
}
.group_info_pop .group_info_content .group_loginOut {
  font-size: 14px;
  color: #db5232;
  cursor: pointer;
  text-align: center;
  width: 100px;
  margin: 15px auto;
  border:1px solid #e1e3e5;
  padding:5px;
  border-radius:6px;
}
.add_icon {
  border: 1px solid #d9dfe6;
  box-sizing: border-box;
  border-radius: 6px;
}
.del_icon {
  border: 1px solid #d9dfe6;
  box-sizing: border-box;
  border-radius: 6px;
}
.add_icon:before {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 2px;
  height: 16px;
  margin-left: -1px;
  margin-top: -8px;
  display: block;
  content: ' ';
  background: #4793f6;
}
.add_icon:after {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 16px;
  height: 2px;
  margin-left: -8px;
  margin-top: -1px;
  display: block;
  content: ' ';
  background: #4793f6;
}
.del_icon:after {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 16px;
  height: 2px;
  margin-left: -8px;
  margin-top: -1px;
  display: block;
  content: ' ';
  background: #4793f6;
}
@keyframes switch_on {
  0% {
    left: 0;
  }
  100% {
    left: 13px;
  }
}
@-webkit-keyframes switch_on {
  0% {
    left: 0;
  }
  100% {
    left: 13px;
  }
}
@keyframes switch_off {
  0% {
    left: 13px;
  }
  100% {
    left: 0;
  }
}
@-webkit-keyframes switch_off {
  0% {
    left: 13px;
  }
  100% {
    left: 0;
  }
}
/* 消息*/
.msg{padding-right:4px;}
.msg .msg_time,.msg .redraw_msg{color:#474747;font-size:12px;text-align:center;padding-bottom:10px;-webkit-user-select:none;-moz-user-select:none;}
.msg .msg_time span,.msg .redraw_msg span{padding:0px 5px;border-radius:4px;background:#dadada;color:#fff;display:inline-block;}
.me{}
.msg .avatar{float:left;width:38px;height:38px;border-radius:4px;overflow:hidden;-webkit-user-select:none;-moz-user-select:none;}
.msg .avatar img{width:100%;height:100%;display:block;-webkit-user-select:none;-moz-user-select:none;cursor:pointer;}
.me .avatar{float:right;}
.emoji_face{width:24px;height:24px;display: inline-block;vertical-align: middle;}

.msg .msg_sender_name{-webkit-user-select: none;-moz-user-select: none;white-space:nowrap;text-align: left;padding-left:15px;font-size: 12px;height: 22px;line-height: 24px;color: #8b8b8b;}
.msg .msg_split{position:absolute;left:0px;width:0px;height:0px;border: 6px solid transparent;border-right: 4px solid #fff;top:13px;}
.me .msg_split{right:0px;left:auto;border: 6px solid transparent;border-left: 4px solid #c8def8;}
.msg .file_msg .msg_split{border-right-color:#d7ddf2;border-left-color:transparent;}
.msg.me .file_msg .msg_split{border-left-color:#d7ddf2;border-right-color:transparent;}
.file_msg .msg_split::before{content:' ';height:0;width:0;position:absolute;border: 6px solid transparent;border-right: 4px solid #fff;left:-5px;top:-6px;}
.me .file_msg .msg_split::before{content:' ';height:0;width:0;position:absolute;border: 6px solid transparent;border-left: 4px solid #fff;right:-5px;top:-6px;}
.msg .msg_txt_warper{position:relative;padding-bottom:15px;margin-left:32px;margin-right:32px;}
.msg .msg_txt_warper::after{display:block;content:' ';clear:both;line-height:0;height:0;font-size:0;}

.msg_txt_warper .resend{position: absolute;right:auto;bottom:5px;left:0;margin-left:-18px;width:22px;height:22px;background: url(../img/file_fail.png) no-repeat center center;}
.msg_txt_warper .sending{position: absolute;right:auto;bottom:5px;left:0;top:50%;margin-top:-4px;margin-left:-15px;width:16px;height:16px;font-size: 0;}
.msg_txt_warper .unread{float:left;width:12px;height:12px;background:#f63d42;border-radius:20px;margin-left: 10px;margin-top:8px;}
.msg_txt .qq_face{background:none;width:24px;height:24px;}

.msg .msg_txt_warper .warper{float:left;position:relative;max-width:88%;}
.msg .media_msg .msg_txt_warper .warper{max-width:60%;}
.me .msg_txt_warper .warper{float:right;}
.msg .msg_txt{margin-left:10px;font-size:14px;min-height:19px;background:#fff;color: #202022;border-radius:4px;padding:8px 9px;word-break: break-all;white-space: normal;overflow: hidden;}
.me .msg_txt{margin-right:10px;background:#c8def8;border-radius:4px;}
.me .msg_txt_warper {margin-right:32px;}
.msg .msg_txt span{white-space:pre-wrap;font-size:14px;word-wrap: break-word;word-break: normal;;}
.msg .msg_txt span.file_status{font-size:13px;}
.me .msg_txt_warper .cancel{position: absolute;right:auto;bottom:-23px;left:0;margin-left:-30px;width:22px;height:22px;background: url(../img/icons.png) no-repeat scroll -374px -357px;}
.hide_name{height:0;width:0;float:left;opacity:0;margin-left:-30px;overflow:hidden;white-space:nowrap!important;}
.offline{filter:grayscale(100%);-webkit-filter:grayscale(100%);}
.is_group .avatar{margin-top:10px;}
.me .avatar{margin-top:0px;}

.file_msg .warper .msg_txt{border:1px solid #d7ddf2;background:#fff;}


.main_content .main_detail .detail_block .chat_detail .chat_content .voice_msg .msg_txt{padding-right:0;padding-left:10px;}
.voice_msg .warper{cursor:pointer;}
.voice_msg .voice_icon{background:url(../img/playicon2.png) no-repeat 0px 0;height:23px;width:23px;display:inline-block;vertical-align:middle;}
.voice_msg .playing{background:url(../img/playing2.gif) no-repeat 1px 0;}
