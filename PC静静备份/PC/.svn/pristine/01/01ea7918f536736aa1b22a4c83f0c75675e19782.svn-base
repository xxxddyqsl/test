/* 大文件传输 */
.large_file .frame_title{top:4px;}
.large_file .confirm{width: 74px;height: 30px;line-height: 30px;border-radius: 4px;font-size: 13px;text-decoration: none;display: block;text-align: center;margin:0 auto;color: #fff;background: linear-gradient(to right,#4692f6,#76bcff);}
.large_file .frame_btns{position:absolute;bottom:10px;left:0;right:0;height:30px;}

.large_file .large_file_title{background:#202022;position:relative;height:30px;color:#fff;}
.large_file .large_file_title .min_frame{top:7px;right:32px;}
.large_file .large_file_title .close_frame{top:7px;right:7px;}
.large_file .drag_tips{color:#999;padding-left:10px;}
.large_file .large_file_middle{position:absolute;top:65px;border-bottom:1px solid #dbdbdb;left:10px;right:10px;bottom:50px;overflow:auto;}

.large_file .large_file_middle .add_file{height:36px;width:36px;border:1px dashed #dcdcdc;position:relative;box-sizing:border-box;display:block;border-radius:2px;margin-top:10px;}
.large_file .large_file_middle .add_file::before{width:2px;height:18px;content:' ';position:absolute;top:50%;left:50%;margin-top:-9px;margin-left:-1px;background:#d2d2d2;}
.large_file .large_file_middle .add_file::after{height:2px;width:18px;content:' ';position:absolute;top:50%;left:50%;margin-left:-9px;margin-top:-1px;background:#d2d2d2;}
.large_file .large_file_middle .add_file:hover{background:#4692f6;border:1px solid #4692f6;}
.large_file .large_file_middle .add_file:hover::before{background:#fff;}
.large_file .large_file_middle .add_file:hover::after{background:#fff;}

.large_file .file_list .file_detail{padding:10px;border:1px solid #dbdbdb;box-sizing:border-box;width:100%;margin-bottom:10px;}
.large_file .file_list .file_detail .file_info{margin-right:40px;}
.large_file .file_list .file_detail .delete_file{color:#e91b19;text-decoration:none;float:right;margin-top:10px;}
.large_file .file_list .file_detail .delete_file:hover{color:#a40c0a;}
.large_file .file_list .file_detail .file_info .file_name{padding-top:4px;}

.large_file .file_list .file_detail .folder_file{background:url(../img/folder_file.png) no-repeat center center;}

.large_file_tips{font-size:12px;color:#999;}

.large_file_status{width:320px;background:#fff;padding:15px;max-height:400px;overflow:auto;}
.large_file_status .avatar{float:left;width:38px;height:38px;border-radius:4px;overflow:hidden;-webkit-user-select:none;-moz-user-select:none;}
.large_file_status .avatar img{width:100%;height:100%;display:block;-webkit-user-select:none;-moz-user-select:none;cursor:pointer;}
.status_block{height:38px;line-height:38px;vertical-align: top;margin-bottom:10px;}
.status_block .receiver_name{margin-left:10px;display:inline-block;vertical-align: top;width:100px;white-space:nowrap;}
.status_block .receiver_status{display:inline-block;margin-left:20px;vertical-align: top;}
.status_block .receive_progress{display:inline-block;height:14px;line-height:14px;position:relative;margin-top:13px;width:100px;margin-left:20px;}
.status_block .receive_progress p{text-align:center;position:absolute;top:0;left:0;right:0;bottom:0;z-index:9;font-size:12px;}
.status_block .receive_progress .receive_progress_complete{position:absolute;left:0;top:0;bottom:0;background:#4692f6;z-index:8;}
