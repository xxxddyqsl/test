.history_right .msg .voice_msg .msg_split{position: absolute; left: 23px;width: 0px;height: 0px; border: 6px solid transparent;border-bottom: 6px solid #d2d2d2; top: -11px;}
.history_right .msg .voice_msg .msg_split::before{content: ' ';height: 0;width: 0;position: absolute;border: 6px solid transparent;border-bottom: 5px solid #fff;left: -6px;top: -4px;}
